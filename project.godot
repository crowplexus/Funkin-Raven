config_version=5

[application]

config/name="Friday Night Funkin' (Raven)"
config/version="1.0.0"
run/main_scene="res://scenes/menu/title_screen.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.svg"

[autoload]

Preferences="*res://scripts/singletons/preferences.gd"
SoundBoard="*res://scenes/tools/sound_board.tscn"
PerformanceCounter="*res://scenes/ui/performance_counter.tscn"
Globals="*res://scripts/singletons/globals.gd"
Transition="*res://scenes/ui/transition/transition.tscn"
Conductor="*res://scripts/singletons/conductor.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[editor]

naming/node_name_num_separator=2
naming/node_name_casing=2

[editor_plugins]

enabled=PackedStringArray("res://addons/parallax_node/plugin.cfg")

